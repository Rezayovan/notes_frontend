{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "S3DeployAccess",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:ListBucket",
                "s3:DeleteObject",
                "cloudfront:CreateInvalidation"
            ],
            "Resource": [
                "arn:aws:s3:::notesai-ui",
                "arn:aws:s3:::notesai-ui/*",
                "arn:aws:cloudfront::292780590924:distribution/*"
            ]
        }
    ]
} 